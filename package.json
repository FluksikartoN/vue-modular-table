{
  "name": "vue-modular-table",
  "version": "0.0.2",
  "description": "Vue2 implementation of data table without jQuery.",
  "author": "eCollect",
  "main": "dist/modular-table.js",
  "license": "MIT",
  "scripts": {
    "dev": "vue build ./src/demo.vue --config ./build.config.js",
    "demo:open": "open 'http://localhost:3000/'; serve ./demo/",
    "dist": "vue build ./src/index.js --config ./build.config.js --dist ./dist/ --prod --lib vue-modular-table",
    "dist:demo": "vue build ./src/demo.vue --config ./build.config.js --dist ./demo/ --prod",
    "docs": "gitbook build ./gitbook ./docs",
    "docs:serve": "gitbook serve ./gitbook ./docs",
    "build": "npm run dist -- --disable-compress",
    "start": "npm run dev"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eCollect/vue-modular-table.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "component",
    "table",
    "datatable"
  ],
  "dependencies": {
    "es5-dot-prop": "^4.1.1"
  }
}
